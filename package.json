{
  "name": "english-trainer",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">= 22"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "TZ=UTC vitest",
    "bench": "TZ=UTC vitest bench",
    "test-info": "DEBUG=vite-node:* npm run test -- --logHeapUsage",
    "//! For debugging open URL - `chrome://inspect` ": "",
    "test-debug": "npm run test -- --inspect-brk --no-file-parallelism",
    "test-update": "npm run test -- -u",
    "coverage": "vitest run --coverage --coverage.reporter='text-summary'",
    "coverage-dev": "vitest run --coverage --coverage.reporter='html'",
    "check-ts": "tsc --noEmit",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "format": "prettier --check src",
    "format-fix": "prettier --write src",
    "knip": "knip --production",
    "lint-all": "npm run check-ts && npm run eslint && npm run format && npm run knip",
    "lint-all-fix": "npm run eslint-fix && npm run format-fix",
    "prepare": "git config core.hooksPath .git-hooks",
    "pre-commit": "npm run lint-all && npm test"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@fontsource/roboto": "^5.1.0",
    "@hookform/resolvers": "^3.9.1",
    "@mui/icons-material": "^6.1.10",
    "@mui/lab": "^6.0.0-beta.11",
    "@mui/material": "^6.1.10",
    "@mui/x-data-grid": "^7.23.1",
    "@tanstack/react-query": "^5.62.3",
    "@tanstack/react-query-devtools": "^5.62.3",
    "@toolpad/core": "^0.10.0",
    "classnames": "^2.5.1",
    "core-js": "^3.39.0",
    "file-saver": "^2.0.5",
    "i18next": "^24.0.5",
    "idb": "^8.0.0",
    "luxon": "^3.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-hotkeys-hook": "^4.6.1",
    "react-router-dom": "^6.28.0",
    "zod": "^3.23.8",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/file-saver": "^2.0.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.2",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.16.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.13.0",
    "happy-dom": "^15.11.7",
    "knip": "^5.39.2",
    "msw": "^2.6.7",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "vite": "^6.0.3",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^2.0.4"
  }
}
